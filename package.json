    "lazy": "~1.0.11",
    "lodash": "^4.0.0",
    "nodegit": "^0.8.0",
    "rimraf": "~2.0.2",
    "underscore": "^1.8.3",
    "underscore.string": "^3.2.2",
    "unzip": "~0.0.4"
